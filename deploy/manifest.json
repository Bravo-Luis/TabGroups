{
  "manifest_version": 3,
  "name": "TabGroups",
  "version": "1.0",
  "description": "An extension to group tabs",
  "permissions": ["tabs"],
  "action": {
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
